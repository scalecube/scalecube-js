{
  "name": "@scalecube/browser",
  "version": "0.0.1",
  "private": false,
  "unpkg": "dist/index.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules && rimraf .cache && rimraf lib && rimraf es && rimraf dist",
    "build": "rimraf .cache && yarn build:dist && yarn build:es && yarn build:cjs",
    "build:dist": "rimraf dist  && rollup -c rollup.iife.config.js",
    "build:es": "rimraf es && rollup -c rollup.es.config.js",
    "build:cjs": "rimraf lib && rollup -c rollup.cjs.config.js",
    "lint": "tslint '{src,tests}/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write '{src,tests}/**/*.{ts,tsx}'",
    "test": ""
  },
  "author": "Scalecube (https://github.com/scalecube/scalecube-js)",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "core-js": "^3.4.2",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-uglify-es": "^0.0.1",
    "rollup-plugin-visualizer": "^2.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@scalecube/api": "^0.0.3-next.4",
    "@scalecube/routers": "^0.0.3-next.10",
    "@scalecube/cluster-browser": "^0.0.3-next.9",
    "@scalecube/transport-browser": "^0.0.3-next.9",
    "@scalecube/utils": "^0.0.3-next.9",
    "@scalecube/scalecube-microservice": "^0.1.4-next.13"
  }
}
