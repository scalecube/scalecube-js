{
  "name": "@scalecube/examples",
  "version": "0.1.2-next.0",
  "private": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "es-bundle",
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules && rimraf .cache && rimraf lib && rimraf es && rimraf dist && rimraf dist-min",
    "build": "rimraf .cache && npm-run-all build:commonJS build:es-module build:es-bundle",
    "build:commonJS": "rimraf lib && tsc --module commonjs --target es5 --outDir lib",
    "build:es-module": "rimraf es-module && tsc --module esnext --target es5 --outDir es",
    "build:es-bundle": "rimraf es-bundle && rimraf .cache && parcel build es/index.js --no-minify --out-dir es-bundle",
    "start-node-example": "yarn run build:commonJS && yarn run open-node-example",
    "open-node-example": "node lib/index.js",
    "lint": "tslint '{src,tests}/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write '{src, tests}/**/*.{ts,tsx}'",
    "runNodejs01": "whoAmI=localHost:1000 seed=localHost:2000 node src/nodejs/index.js -port 1000",
    "runNodejs02": "whoAmI=localHost:2000 node src/nodejs/index.js -port 2000",
    "runNodejs03": "whoAmI=localHost:3000 seed=localHost:2000 node src/nodejs/index.js -port 3000",
    "runNodejs04": "whoAmI=localHost:4000 seed=localHost:3000 node src/nodejs/index.js -port 4000",
    "runNodejs05": "whoAmI=localHost:5000 seed=localHost:2000 node src/nodejs/index.js -port 5000",
    "runNodejs06": "whoAmI=localHost:6000 seed=localHost:5000 node src/nodejs/index.js -port 6000",
    "runNodejs07": "whoAmI=localHost:7000 seed=localHost:5000 node src/nodejs/index.js -port 7000"
  },
  "author": "Scalecube (https://github.com/scalecube/scalecube-js)",
  "devDependencies": {
    "@types/node": "10.1.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.4",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@scalecube/api": "^0.0.3-next.0",
    "@scalecube/scalecube-microservice": "^0.1.4-next.0",
    "@scalecube/transport-nodejs": "^0.0.3-next.0",
    "@scalecube/utils": "^0.0.3-next.0"
  }
}
